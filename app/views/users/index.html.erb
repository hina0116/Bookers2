<%= render 'books/info', user: current_user %>
<%= render 'books/newbook', book: @book %>

<h1>Users</h1>
  <table>
    <thead>
      <tr>
        <th>image</th>
        <th>name</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
      <tr>
        <td><%= image_tag user.get_profile_image(100,100) %></td>
        <td><%= user.name %></td>
        <td><%= link_to 'Show', user_path(@users) %></td>
      </tr>
      <% end %>
    </tbody>
  </table>